{
  "testEnvironment": "node",
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/server.js",
    "!src/config/**",
    "!src/utils/fixAccountIndex.js"
  ],
  "testMatch": [
    "**/__tests__/**/*.test.js",
    "**/?(*.)+(spec|test).js"
  ],
  "transform": {},
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
  "testTimeout": 10000
}
